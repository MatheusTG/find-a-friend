{
  "$schema": "https://turbo.build/schema.json",
  "envMode": "strict",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start:dev": {
      "persistent": true,
      "cache": false
    },
    "prisma:generate": {
      "outputs": ["src/generated/prisma/**"]
    },
    "test:unit": {
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["prisma:generate"],
      "cache": false
    },
    "format": {},
    "format:check": {}
  }
}
